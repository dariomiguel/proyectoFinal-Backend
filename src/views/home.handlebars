<h1>Lista de productos</h1>

{{#each result.products}}
    <div>
        <p>{{this.title}}</p>
        <p>$ {{this.price}}</p>
        <p>N° Id: {{this.id}}</p>
        <p>{{this.description}}</p>
        <p>Código de producto: {{this.code}}</p>
        <p>Cant. Stock:{{this.stock}}</p>
        <p>Categoría: {{this.category}}</p>
        <p>{{this.thumbnail}}</p>
        <hr />
    </div>
{{/each}}

Total productos :
{{result.totalDocs}}<br />
pages
{{result.page}}
/
{{result.totalPages}}<br />

{{#if result.hasPrevPage}}
    <a href="/?page={{result.prevPage}}&limit{{result.limit}}">Previous</a>
{{/if}}
{{#if result.hasNextPage}}
    <a href="/?page={{result.nextPage}}&limit{{result.limit}}">Next</a>
{{/if}}

<script src="/socket.io/socket.io.js"></script>
<script src="/js/home.js"></script>