<div id="tarjeta">
    <h1>Detalles de la Tarjeta de Crédito</h1>
    <p>Número de Tarjeta: {{numero}}</p>
    <p>Titular: {{titular}}</p>
    <p>Fecha de Expiración: {{expiracion}}</p>
    <p>Código de Seguridad: {{codigo}}</p>
</div>

<script id="tarjeta-template" type="text/x-handlebars-template">
    <h1>Detalles de la Tarjeta de Crédito</h1> <p>Número de Tarjeta:
    {{numero}}</p> <p>Titular:
    {{titular}}</p> <p>Fecha de Expiración:
    {{expiracion}}</p> <p>Código de Seguridad:
    {{codigo}}</p>
</script>

<script
    src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.7.7/handlebars.min.js"
></script>
<script>
    // Datos de la tarjeta de crédito let tarjetaData = { numero: '1234 5678
    9012 3456', titular: 'Nombre del Titular', expiracion: '01/25', codigo:
    '123' }; // Compilar el template let tarjetaTemplate =
    document.getElementById('tarjeta-template').innerHTML; let template =
    Handlebars.compile(tarjetaTemplate); // Renderizar el template con los datos
    de la tarjeta document.getElementById('tarjeta').innerHTML =
    template(tarjetaData);
</script>